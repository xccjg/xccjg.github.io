<script type="text/javascript">
function trimAll(str){
    var rst = '';
    if(str && str.length > 0){
        rst = str.replace(/(^\s*)|(\s*$)/g, "");
    }
    return rst;
}
$(function(){
    //搜索
    $('#searchTypeMenu a').click(function(){
        var keyword = trimAll($('#searchKeyword').val());
        var texts = ['输入产品名称','输入公司名称','输入求购名称'];
        var types = ['http://www.npicp.com/search','http://www.npicp.com/search/company','http://www.npicp.com/search/buy'];
        var i = $(this).index();
        $('#searchForm').attr('action',types[i]);
        if( keyword.length ){
            $('#searchKeyword').attr('placeholder',texts[i]);
        }
        $(this).addClass('active').siblings().removeClass('active');
    });
    $('#searchBtn').click(function(){
        var keyword = trimAll($('#searchKeyword').val());
        if( keyword.length==0 ){
            alert('请输入关键词！');
            $('#searchKeyword').focus();
            return false;
        } else {
            $('#searchForm').submit();
        }
    });
    $('#searchBtnBottom').click(function(){
        var keyword = trimAll($('#searchKeywordBottom').val());
        if( keyword.length==0 ){
            alert('请输入关键词！');
            $('#searchKeywordBottom').focus();
            return false;
        } else {
            $('#searchFormBottom').submit();
        }
    });
    $('#searchBtnCompany').click(function(){
        var keyword = trimAll($('#swd').val());
        if( keyword.length==0 ){
            alert('请输入关键词');
            $('#swd').focus();
            return false;
        } else {
            $('#searchFormCompany').submit();
        }
    });
});
</script>
